pipeline {
    agent any

    environment {
        IMAGE_NAME = "sicei"
    }

    stages {
        stage('Checkout'){
            steps {
                echo "Checking out..."
            }
        }
        stage('Build') {
            steps {
                echo "Building"
            }
        }
        stage('Run'){
            steps {
                echo "Running..."
            }
        }
    }
}